ffmchar("U",15,ht#,0);
italcorr ht#*slant;
x1=leftstemloc+noise; 
x2=leftstemloc+noise; 
w-x4=leftstemloc+noise; 
w-x5=leftstemloc+noise; 
x3=.5[x1,x4]+noise;
top y1=h+o+noise;
y2=barheight+noise;
bot y3=noise-o;
y4=barheight+noise;
top y5=h+o+noise;
draw z1--z2
 & half(z2,z2-z1,z3,randrt,z4,z5-z4)
 & z4--z5; 
charanchortops_[charcode]=(x3,h);
labels(1,2,3,4,5); 
endchar;
