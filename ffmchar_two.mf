ffmchar("two",11,ht#,0);
italcorr .8ht#*slant;
x1=leftstemloc-ho+noise; 
x2=.5[x1,x6]+noise;
w-x3=good.x(1.5u+s+noise);
x4=.5[x1,x6]+noise;
x5=good.x(2u+s+noise);
x6=.5[w-x1,lft w]+ho+noise;
top y2=h+noise;
y3=.5[y4,y2]+noise;
y4=barheight+noise; 
bot y5=noise; 
bot y6=noise; 
z1=z2+whatever*randrt;
pair randir;
randir:=-randrt;
draw z1--z2 
 & half(z2,z2-z1,z3,-randup,z4,randir)
 & arc(z4,randir,z5,-randup)
 & z5--z6;
labels(1,2,3,4,5,6); 
endchar;
